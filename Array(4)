	public static void main(String[] args) {

		char a[] = { 'F', 'A', 'Y', 'S', 'A', 'L' };
		for (int i = 0; i < a.length; i++) {
			System.out.println(a[i]);
		}
		System.out.println('\n');
		for (int i = 0; i < a.length; i++) {
			System.out.print(a[i]);
		}
		System.out.println();
		System.out.println(a);

	}
